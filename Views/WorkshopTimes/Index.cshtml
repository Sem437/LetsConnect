@model IEnumerable<LetsConnect.Models.WorkshopTimes>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.WorkshopId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkshopRonde)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkshopPlace)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkshopDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkshopStartTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkshopEndTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkshopTeacher)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.WorkshopId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WorkshopRonde)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WorkshopPlace)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WorkshopDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WorkshopStartTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WorkshopEndTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WorkshopTeacher)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.WorkshopTimeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.WorkshopTimeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.WorkshopTimeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
